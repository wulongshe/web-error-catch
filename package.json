{
  "name": "web-error-catch",
  "version": "0.0.0",
  "description": "在无source map的生产环境中，将web error信息交由server端正确处理",
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint-staged": "lint-staged",
    "prettier": "npx --no-install pnpm recursive run prettier",
    "test": "npx --no-install pnpm recursive run test",
    "build": "npx --no-install pnpm recursive run build",
    "ac": "git add . && cz",
    "cm": "cz"
  },
  "lint-staged": {
    "packages/**/*.{js,ts}": [
      "npm run lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "keywords": [
    "source map",
    "error catch"
  ],
  "author": "shewulong",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
